{
  "name": "@elastic/request-converter",
  "version": "0.1.0",
  "description": "Elasticsearch request converter",
  "author": "Miguel Grinberg",
  "license": "Apache-2.0",
  "type": "commonjs",
  "bin": {
    "es-request-converter": "./dist/es-request-converter.js"
  },
  "scripts": {
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.js src/**/*.tpl dist/",
    "docs": "typedoc src/index.ts",
    "lint": "eslint src tests",
    "prettier": "prettier \"src/**/*.ts\" \"tests/**/*.ts\" --list-different",
    "test": "jest --test-path-ignore-patterns integration --coverage",
    "test:setup": "node scripts/download-examples.mjs && ./tests/integration/run-python.sh",
    "test:integration": "jest tests/integration",
    "test:example": "./scripts/test-example.sh",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "fix:lint": "eslint src tests --fix",
    "fix:prettier": "prettier \"src/**/*.ts\" \"tests/**/*.ts\" --write",
    "fix:precommit": "./scripts/precommit.sh",
    "watch:build": "tsc -w",
    "watch:test": "jest --test-path-ignore-patterns integration --coverage --watch",
    "prepare": "husky"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.3",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "dependencies": {
    "@types/child-process-promise": "^2.2.6",
    "@types/commander": "^2.12.2",
    "child-process-promise": "^2.2.1",
    "commander": "^12.1.0",
    "find-my-way-ts": "^0.1.2",
    "handlebars": "^4.7.8",
    "querystring": "^0.2.1"
  }
}
