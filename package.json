{
  "name": "@elastic/request-converter",
  "version": "0.1.0",
  "description": "Elasticsearch request converter",
  "author": "Miguel Grinberg",
  "license": "Apache-2.0",
  "type": "commonjs",
  "scripts": {
    "test": "npm run build && npm run lint && npm run prettier && jest --coverage",
    "build": "tsc",
    "lint": "eslint src tests",
    "prettier": "prettier \"src/**/*.ts\" \"tests/**/*.ts\" --list-different",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "fix:lint": "eslint src tests --fix",
    "fix:prettier": "prettier \"src/**/*.ts\" \"tests/**/*.ts\" --write",
    "watch:build": "tsc -w",
    "watch:test": "jest --coverage --watch"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "dependencies": {
    "find-my-way-ts": "^0.1.2",
    "querystring": "^0.2.1"
  }
}
